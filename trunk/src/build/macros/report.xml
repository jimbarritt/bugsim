<?xml version="1.0" encoding="UTF-8"?>
<project name="report">

    <macrodef name="echo-classpath">
            <attribute name="path.refid"/>
            <sequential>
                <property name="classpath.out" refid="@{path.refid}"/>
                <echo message="@{path.refid}     : ${classpath.out}"/>
            </sequential>
    </macrodef>

    
</project>